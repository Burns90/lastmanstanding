{
  "projects": {
    "default": "last-man-standing-6cc93"
  },
  "targets": {
    "last-man-standing-6cc93": {
      "hosting": {
        "web": ["last-man-standing-6cc93"]
      }
    }
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": [
    {
      "target": "web",
      "public": "web/public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": ["node_modules", ".git", "firebase-debug.log", "build/**"]
    }
  ]
}
